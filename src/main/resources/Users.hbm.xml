<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.User" table="USERS" schema="USERS_DB" catalog="MYDB">

        <id name="id">
            <column name="ID" sql-type="integer(10)"/>
        </id>

        <property name="userName">
            <column name="USER_NAME" sql-type="varchar(255)"/>
        </property>

        <property name="userPasw">
            <column name="USER_PASW" sql-type="varchar(255)"/>
        </property>

    </class>
</hibernate-mapping>